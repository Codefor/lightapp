/*网页样式重置reset*/
html { font-size:1em;-webkit-tap-highlight-color:rgba(0,0,0,0); -webkit-tap-highlight:rgba(0,0,0,0);-webkit-text-size-adjust:none;}
body { font-size:0.75em;}
label { cursor:pointer}
a:link, a:visited { text-decoration:none}
input,button,select,textarea{outline:none} textarea{resize:none;}

a, abbr, acronym, address, applet, article, aside, audio, b, blockquote, big, body, center, canvas, caption, cite, code, command, datalist, dd, del, details, dfn, dl, div, dt, em, embed, fieldset, figcaption, figure, font, footer, form, h1, h2, h3, h4, h5, h6, header, hgroup, html, i, iframe, img, ins, kbd, keygen, label, legend, li, meter, nav, menu, object, ol, output, p, pre, progress, q, s, samp, section, small, span, source, strike, strong, sub, sup, table, tbody, tfoot, thead, th, tr, tdvideo, tt,
u, ul, var { margin:0; padding:0;font-style:normal}

article, aside, footer, header, hgroup, nav, section, figure, figcaption { display: block} /*html5设置*/

h1, h2, h3, h4, h5, h6, th, td, table, input, button, select, textarea, sub{ font-size:1em}
body, input, button, select, textarea, sub{ font-family:Arial, sans-serif}
em, cite, address, optgroup { font-style:normal}
kbd, samp, code { font-family:monospace}

img, input, button, select, textarea { vertical-align:middle}
ul, ol { list-style:none}
img, fieldset { border:0}
abbr, acronym { cursor:help; border-bottom:1px dotted black}
table {	width:100%; border-spacing:0; border:0}
table th, table td { border:0}
legend, hr { overflow:hidden; position:absolute; top:0; left:0}
legend, hr, caption { visibility:hidden; font-size:0; width:0; height:0; line-height:0}

/*------------------------------------------------------------------------------------------------------------------------------*/
/*统一样式*/
html {width:100%;height:100%;}
body {margin:0 auto;width:640px;height:100%;background:#fff; font-family:"微软雅黑";overflow-x:hidden;}
.load{background:#fff;width:100%;height:100%;position:absolute;top:0;left:0;z-index:100;opacity:1;z-index:10;}
.load img{height:88px;left:50%;position:absolute;top:240px;width: 88px;margin-left:-44px;}
.load .text{color:#787878;font-family: Arial,Helvetica,sans-serif;font-size:24px;font-weight:normal;height:2.5%;left:50%;position:absolute;text-align:center;text-decoration:none;top:350px;width:110px;margin-left:-55px;}
section{height:670px;}
header{height:322px;background:url('../images/1.png') no-repeat;position:relative;z-index:1;}
header .title{width:161px;height:130px;background:url('../images/6.png') no-repeat;right:35px;top:-130px;position:absolute;}
header .judge{background:url('../images/15.png') no-repeat;}
header .t{width:63px;height:63px;position:absolute;top:25px;left:35px;text-align:center;line-height:63px;font-size:40px;color:#000;}
header .topic{width:572px;height:329px;background:url('../images/2.jpg') no-repeat;position:absolute;left:50%;bottom:500px;margin:0 0 -164px -286px;}
header .topic .topicbox{width:100%;height:100%;display:table;vertical-align:middle;}
header .topic .topiccon{width:100%;display:table-cell;vertical-align:middle;}
header .topic .topiccon span{display:block;margin:0 9px;padding:20px 5px;background:rgba(0, 0, 0, .5);color:#fff;font-size:30px;line-height:50px;opacity:0;}
header .topic .topiccon .introduction{padding-left:25px;font-size:22px;line-height:25px}
header .topic .topiccon .introduction h2{font-size:34px;font-weight:700;line-height:40px}
header .topic .topiccon .introduction h3{font-size:32px;line-height:40px}
header .topic .topiccon .introduction em{color:#fcd5b5;}
.score{height:333px;border-bottom:10px solid #f08300;position:relative;display:none;}
.score span{width:121px;height:123px;position:absolute;bottom:-40px;right:80px;background:url('../images/14.png') no-repeat;}
.score .scores{height:333px;}
.score .scores em{display:block;font-size:40px;text-align:center;line-height:80px;padding:10px 0}
.score .scores dfn{display:block;font-size:140px;text-align:center;}
.score .scores.bg{background:url('../images/11.png') no-repeat;}
.score .scores.bg dfn{width:307px;height:98px;position:absolute;top:118px;left:172px;text-align:center;line-height:98px;font-size:80px;}
.con{width:640px;height:670px;position:absolute;top:322px;overflow:hidden;}
.con .go{width:194px;height:84px;background:url('../images/3.png') no-repeat;position:absolute;left:50%;top:290px;margin:0 0 0 -97px;opacity:0;}
.con .index-b{width:640px;height:209px;background:url('../images/4.png') no-repeat;position:absolute;left:0%;bottom:-168px;margin:0 0 0 0px;}
.con .list{position:absolute;top:250px;left:-593px;width:593px;}
.con .list ul li{width:558px;height:auto;margin:0 0 12px 0;background:url('../images/5.png') no-repeat;padding-left:32px;text-align:left;float:left}
.con .list ul li span{float:left;height:95px;width:95px;line-height:95px;text-align:center;font-size:28px;color:#fff;}
.con .list ul li em{float:left;width:415px;line-height:30px;text-align:center;font-size:28px;color:#fff;display:table;}
.con .list ul li em dfn{width:415px;height:95px;display:table-cell;vertical-align: middle;}
.con .list ul li em dfn cite{display:block;}
.con .y{width:0;height:0;position:absolute;top:322px;left:182px;background:url('../images/7.png') no-repeat center; background-size:100% 100%;overflow:hidden;}
.con .n{width:0;height:0;position:absolute;top:322px;right:182px;background:url('../images/8.png') no-repeat center; background-size:100% 100%;overflow:hidden;}
.con .title{width:100%;text-align:center;position:absolute;top:40%;font-size:50px;line-height:65px;opacity:0;display:none;}
.con .shenma{width:221px;height:226px;background:url('../images/9.png') no-repeat;position:absolute;left:50%;top:40%;margin:0 0 0 -110px;display:none;}
.con .logo{height:57px;position:absolute;top:100px;left:50px;opacity:0;display:none;}
.con .rcode{width:540px;position:absolute;top:185px;left:50px;opacity:0;display:none;}
.con .rcode .int{font-size:30px;line-height:40px;padding-left: 148px;}
.con .rcode .code{text-align:center;}
.footer{background:url('../images/10.png') no-repeat bottom;}
